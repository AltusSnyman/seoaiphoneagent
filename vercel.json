{
  "redirects": [
    {
      "source": "https://www.aiphoneagent.ai/(.*)",
      "destination": "https://aiphoneagent.ai/$1",
      "permanent": true
    },
    {
      "source": "https://www.aiphoneagent.com/(.*)",
      "destination": "https://aiphoneagent.ai/$1", 
      "permanent": true
    },
    {
      "source": "https://aiphoneagent.com/(.*)",
      "destination": "https://aiphoneagent.ai/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        }
      ]
    }
  ]
}