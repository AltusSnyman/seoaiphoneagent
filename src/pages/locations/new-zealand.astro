      "New Zealand Accent Voice AI", 
      "Local Business Hour Awareness",
      "NZ Holiday Schedule Integration",
      "Multi-language Support (English, Te Reo Māori)",
      "GST Compliance Ready",
      "NZDT/NZST Time Zone Optimization"
    ]}
    targetAudience="New Zealand Business Owners"
    availableHours="24/7 across all NZ time zones"
  />
  
